{
  "name": "learntocodejs",
  "version": "0.0.0-semamtically-released",
  "description": "learn to code javascript",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "test": "mocha src/index.test.js -w",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nidagil/Js.git"
  },
  "keywords": [
    "js"
  ],
  "author": "Panida <panida.gilbert@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nidagil/Js/issues"
  },
  "homepage": "https://github.com/nidagil/Js#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.2",
    "istanbul": "0.4.5",
    "mocha": "4.1.0",
    "semantic-release": "^12.2.2",
    "travis-deploy-once": "^4.3.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
